@model IEnumerable<Garage2.Models.Vehicle>

@{
    ViewBag.Title = "Index";
}

<h2>Parked Vehicles</h2>


<table class="table">
	<tr>
		<th>
			@Html.DisplayNameFor(model => model.Reg)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Type)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Brand)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Color)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Wheels)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.CheckIn)
		</th>
		<th></th>
	</tr>

@foreach (var item in Model) {
	<tr>
		<td>
			@Html.DisplayFor(modelItem => item.Reg)
		</td>
		<td>
			@Html.DisplayFor(modelItem => item.Type)
		</td>
		<td>
			@Html.DisplayFor(modelItem => item.Brand)
		</td>
		<td>
			@Html.DisplayFor(modelItem => item.Color)
		</td>
		<td>
			@Html.DisplayFor(modelItem => item.Wheels)
		</td>
		<td>
			@Html.DisplayFor(modelItem => item.CheckIn)
		</td>
		<td>
			@Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
			@Html.ActionLink("Details", "Details", new { id = item.Id }) |
			@Html.ActionLink("Check out", "Delete", new { id = item.Id })
		</td>
	</tr>
}

</table>
